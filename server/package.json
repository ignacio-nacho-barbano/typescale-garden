{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm i & npx tsc",
    "start": "node dist/src/index.js",
    "dev": "nodemon --watch './src' -e .ts --exec ts-node -r ./env-loader.dev.js ./src/index.ts",
    "db:migrate:dev": "dotenv -e ../.env npx prisma prisma db push seed",
    "db:migrate:prod": "dotenv -e ../.env.prod npx prisma db push seed",
    "prisma:file": "ts-node --project ../tsconfig.json -r ./env-loader.prod.js ./prisma/seeds/typescales.ts"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seeds/typescales.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.9.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-oauth2-jwt-bearer": "^1.6.0",
    "express-openid-connect": "^2.17.1",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "mongodb": "^6.3.0",
    "morgan": "~1.9.1",
    "prisma": "^5.9.1",
    "rollbar": "^2.26.3",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.16",
    "dotenv-cli": "^7.3.0",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2"
  }
}